{            
    "request": {
        "method": "POST",
        "urlPattern": "/v2/measure",
        "headers": {
            "Authorization": {
                "matches": "Bearer dummy_token"
            },
            "Content-Type": {
                "matches": "application/x-www-form-urlencoded"
            }
        },
        "bodyPatterns": [
            {
                "matches": "(.*&|^)action=getintradayactivity($|&.*)"
            },
            {
                "matches": "(.*&|^)startdate=2021-01-01($|&.*)"
            },
            {
                "matches": "(.*&|^)enddate=2021-01-02($|&.*)"
            },
            {
                "matches": "(.*&|^)data_fields=steps,elevation,calories($|&.*)"
            }
        ]
    },
    "response": {
        "status": 200,
        "bodyFileName": "Measure/Measurev2-Getintradayactivity.json",
        "headers": {
            "Content-Type": "application/json"
        }
    }
}