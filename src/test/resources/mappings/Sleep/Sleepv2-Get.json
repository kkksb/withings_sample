{            
    "request": {
        "method": "POST",
        "urlPattern": "/v2/sleep",
        "headers": {
            "Authorization": {
                "matches": "Bearer dummy_token"
            },
            "Content-Type": {
                "matches": "application/x-www-form-urlencoded"
            }
        },
        "bodyPatterns": [
            {
                "matches": "(.*&|^)action=get($|&.*)"
            },
            {
                "matches": "(.*&|^)startdate=2021-01-01($|&.*)"
            },
            {
                "matches": "(.*&|^)enddate=2021-01-02($|&.*)"
            }
        ]
    },
    "response": {
        "status": 200,
        "bodyFileName": "Sleep/Sleepv2-Get.json",
        "headers": {
            "Content-Type": "application/json"
        }
    }
}