{
    "request": {
        "method": "POST",
        "urlPattern": "/v2/user",
        "headers": {
            "Authorization": {
                "doesNotMatch": "Bearer ($|&.*)"
            },
            "Content-Type": {
                "matches": "application/x-www-form-urlencoded"
            }
        },
        "bodyPatterns": [
            {
                "matches": "(.*&|^)action=link($|&.*)"
            },
            {
                "matches": "(.*&|^)client_id=test_client_id($|&.*)"
            },
            {
                "matches": "(.*&|^)nonce=test_nonce($|&.*)"
            },
            {
                "matches": "(.*&|^)signature=test_signature_sample($|&.*)"
            },
            {
                "matches": "(.*&|^)email=test_email($|&.*)"
            }
        ]
    },
    "response": {
        "status": 200,
        "bodyFileName": "User/Userv2-Unlink.json",
        "headers": {
            "Content-Type": "application/json"
        }
    }
}