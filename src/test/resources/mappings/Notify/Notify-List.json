{            
    "request": {
        "method": "POST",
        "urlPattern": "/notfiy",
        "headers": {
            "Authorization": {
                "matches": "Bearer dummy_token"
            },
            "Content-Type": {
                "matches": "application/x-www-form-urlencoded"
            }
        },
        "bodyPatterns": [
            {
                "matches": "(.*&|^)action=list($|&.*)"
            },
            {
                "matches": "(.*&|^)appli=12345($|&.*)"
            }
        ]
    },
    "response": {
        "status": 200,
        "bodyFileName": "Notify/Notify-List.json",
        "headers": {
            "Content-Type": "application/json"
        }
    }
}